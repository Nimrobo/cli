{
  "name": "@nimrobo/cli",
  "version": "0.1.1",
  "description": "CLI tool for interacting with Nimrobo AI APIs - Voice Screening & Matching Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nimrobo": "./bin/nimrobo"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:int": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js"
  },
  "files": [
    "dist",
    "bin",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "nimrobo",
    "cli",
    "voice",
    "interview",
    "ai",
    "screening",
    "network",
    "matching"
  ],
  "author": "Nimrobo",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nimrobo/cli.git"
  },
  "homepage": "https://nimrobo.com",
  "bugs": {
    "url": "https://github.com/nimrobo/cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "inquirer": "^8.2.6",
    "tabtab": "^3.0.2"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "axios-mock-adapter": "^1.22.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
